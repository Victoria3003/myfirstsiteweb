utton>\n  </div>\n</template>\n\n<script>\nimport firebase from \"firebase\";\n\nexport default {\n  data() {\n    return {\n      user: null,\n    };\n  },\n  created() {\n    firebase.auth().onAuthStateChanged((user) => {\n      if (user) {\n        this.user = user;\n      } else {\n        this.user = null;\n      }\n    });\n  },\n  methods: {\n    logOut() {\n      firebase\n        .auth()\n        .signOut()\n        .then(() => {\n          firebase.auth().onAuthStateChanged(() => {\n            this.$router.push(\"/\");\n          });\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}