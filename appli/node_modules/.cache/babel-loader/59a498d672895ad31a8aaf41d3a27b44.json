,KAAK,EAAC,cAYC;AAXP,IAAA,EAAE,EAAC,SAWI;;aAVE,KAAA,CAAA,OAAA,CAAQ,OAAR,GAAe,M;MAUjB;AATP,IAAA,QAAQ,EAAR;AASO,GAbT,E,CAME,W,EACA,W,EACA,W,EACA,W,EACA,W,EACA,W,EACA,W,CAZF,E;;AAAA,G,mBAGW,KAAA,CAAA,OAAA,CAAQ,O,GAWf,CAhBN,CA8CI,EA7BJ,YAAA,CAgBM,KAhBN,EAAA,WAAA,EAgBM,CAfJ,WAeI,E,gBAdJ,YAAA,CAaS,QAbT,EAaS;AAZP,IAAA,KAAK,EAAC,cAYC;AAXP,IAAA,EAAE,EAAC,KAWI;;aAVE,KAAA,CAAA,OAAA,CAAQ,GAAR,GAAW,M;MAUb;AATP,IAAA,QAAQ,EAAR;AASO,GAbT,E,CAME,W,EACA,W,EACA,W,EACA,W,EACA,W,EACA,W,EACA,W,CAZF,E;;AAAA,G,mBAGW,KAAA,CAAA,OAAA,CAAQ,G,GAWf,CAhBN,CA6BI,EAZF,YAAA,CAWI,KAXJ,EAAA,WAAA,EAWI,CAVJ,WAUI,E,gBATJ,YAAA,CAQS,QART,EAQS;AAPP,IAAA,KAAK,EAAC,cAOC;AANP,IAAA,EAAE,EAAC,eAMI;;aALE,KAAA,CAAA,OAAA,CAAQ,MAAR,GAAc,M;MAKhB;AAJP,IAAA,QAAQ,EAAR;AAIO,GART,E,CAME,W,EACA,W,CAPF,E;;AAAA,G,mBAGW,KAAA,CAAA,OAAA,CAAQ,M,GAMf,CAXJ,CAYE,CArJN,C,EAsJA,W,CAvJF,E;;AAAA,GAyJI,CA3JN,CA4JI,CA7JN,C","sourcesContent":["<template>\n  <div class=\"justify-content-center container\">\n    <div class=\"col-md-12\">\n      <h3 class=\"text-center\">Mise à jour de Annonce</h3>\n      <form @submit.prevent=\"onUpdateForm\">\n        <div class=\"form-row\">\n          <div class=\"col-md-4 mb-1\">\n            <label for=\"annonceType\">Achat / Vente</label>\n            <select\n              class=\"form-control\"\n              id=\"annonceType\"\n              v-model=\"annonce.av\"\n              required\n            >\n              <option>Achat</option>\n              <option>Vente</option>\n            </select>\n          </div>\n          <div class=\"col-md-4 mb-1\">\n            <label for=\"annonceMv\">Maison / Appt</label>\n            <select\n              class=\"form-control\"\n              id=\"annonceMv\"\n              v-model=\"annonce.maison\"\n              required\n            >\n              <option>Maison</option>\n              <option>Appartement</option>\n            </select>\n          </div>\n          <div class=\"col-md-4 mb-1\">\n            <label>Prix</label>\n            <input\n              class=\"form-control\"\n              id=\"prix\"\n              v-model=\"annonce.prix\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-2\">\n            <label>Ville</label>\n            <input\n              class=\"form-control\"\n              id=\"ville\"\n              v-model=\"annonce.ville\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-2\">\n            <label>CP</label>\n            <input class=\"form-control\" id=\"cp\" v-model=\"annonce.cp\" required />\n          </div>\n          <div class=\"col-md-4 mb-2\">\n            <label>Surface</label>\n            <input\n              class=\"form-control\"\n              id=\"surface\"\n              v-model=\"annonce.surface\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Pieces</label>\n            <input\n              class=\"form-control\"\n              id=\"pieces\"\n              v-model=\"annonce.pieces\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Chambres</label>\n            <input\n              class=\"form-control\"\n              id=\"chambres\"\n              v-model=\"annonce.chambres\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Année</label>\n            <input\n              class=\"form-control\"\n              id=\"annee\"\n              v-model=\"annonce.annee\"\n              required\n            />\n          </div>\n          <div class=\"col-md-6 mb-4\">\n            <label for=\"contactComment\">Descriptif :</label>\n            <textarea\n              class=\"form-control\"\n              id=\"contactComment\"\n              rows=\"3\"\n              v-model=\"annonce.commentaire\"\n              required\n            ></textarea>\n          </div>\n          <div class=\"col-md-6 mb-4\">\n            <label for=\"equipement\">Equipement :</label>\n            <textarea\n              class=\"form-control\"\n              id=\"equipement\"\n              rows=\"3\"\n              v-model=\"annonce.equipement\"\n              required\n            ></textarea>\n          </div>\n          <div class=\"col-md-4 mb-5\">\n            <label for=\"energie\">Energie</label>\n            <select\n              class=\"form-control\"\n              id=\"energie\"\n              v-model=\"annonce.energie\"\n              required\n            >\n              <option>A</option>\n              <option>B</option>\n              <option>C</option>\n              <option>D</option>\n              <option>E</option>\n              <option>F</option>\n              <option>G</option>\n            </select>\n          </div>\n          <div class=\"col-md-4 mb-5\">\n            <label for=\"ges\">Ges</label>\n            <select\n              class=\"form-control\"\n              id=\"ges\"\n              v-model=\"annonce.ges\"\n              required\n            >\n              <option>A</option>\n              <option>B</option>\n              <option>C</option>\n              <option>D</option>\n              <option>E</option>\n              <option>F</option>\n              <option>G</option>\n            </select>\n          </div>\n            <div class=\"col-md-4 mb-1\">\n            <label for=\"annonceStatut\">Statut</label>\n            <select\n              class=\"form-control\"\n              id=\"annonceStatut\"\n              v-model=\"annonce.statut\"\n              required\n            >\n              <option>A traiter</option>\n              <option>Traité</option>\n            </select>\n          </div>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Mettre a jour</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { db } from \"../firebaseDb\";\n\nexport default {\n  data() {\n    return {\n      annonce: {},\n    };\n  },\n  created() {\n    let dbRef = db.collection(\"annonces\").doc(this.$route.params.id);\n    dbRef\n      .get()\n      .then((doc) => {\n        this.annonce = doc.data();\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    onUpdateForm(event) {\n      event.preventDefault();\n      db.collection(\"annonces\")\n        .doc(this.$route.params.id)\n        .update(this.annonce)\n        .then(() => {\n          console.log(\"Annonce successfully updated!\");\n          this.$router.push(\"/adminAnnonces\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}