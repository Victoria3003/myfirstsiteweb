e carousel -->\n      <div class=\"row\">\n        <div class=\"col-lg-12\">\n          <br />\n          <h2>\n            {{ annonce.av }} {{ annonce.maison }} à {{ annonce.ville }} -\n            {{ annonce.chambres }} chambres\n          </h2>\n          <br />\n          <img src=\"../assets/immo1.jpg\" />\n          <br />\n          <br />\n          <h3>Au prix de {{ annonce.prix }} €</h3>\n          <br />\n        </div>\n      </div>\n      <div class=\"row featurette\">\n        <div class=\"col-sm\">\n          <p class=\"lead\">\n            Nombre de pièces : {{ annonce.pieces }}\n            <br />\n            Nombre de chambres : {{ annonce.chambres }}\n            <br />\n            Année de construction : 1970\n            <br />\n          </p>\n        </div>\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo2.jpg\" />\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <div class=\"row featurette\">\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo3.jpg\" />\n        </div>\n        <div class=\"col-sm\">\n          <p class=\"lead\">\n            Descriptif : <br />\n            {{ annonce.commentaire }}\n          </p>\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <div class=\"row featurette\">\n        <div class=\"col-sm\">\n          <p class=\"lead\">\n            Equipement : <br />\n            {{ annonce.equipement }}\n          </p>\n        </div>\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo4.jpg\" />\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <div class=\"row featurette\">\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo5.jpg\" />\n        </div>\n        <div class=\"col-sm\">\n          <p class=\"lead\">\n            Bilan Energetique :<br />\n            Consommation d'énérgie : {{ annonce.energie }} <br />\n            Emission de Gaz à effet de serre : {{ annonce.ges }}\n          </p>\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <!-- /END THE FEATURETTES -->\n    </div>\n    <!-- /.container -->\n  </main>\n</template>\n\n<script>\nimport { db } from \"../firebaseDb\";\n\nexport default {\n  data() {\n    return {\n      annonce: {},\n    };\n  },\n  created() {\n    let dbRef = db.collection(\"annonces\").doc(this.$route.params.id);\n    dbRef\n      .get()\n      .then((doc) => {\n        this.annonce = doc.data();\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n};\n</script>\n\n<style>\n.bd-placeholder-img {\n  font-size: 1.125rem;\n  text-anchor: middle;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n}\n\n@media (min-width: 768px) {\n  .bd-placeholder-img-lg {\n    font-size: 3.5rem;\n  }\n}\n</style>\n"],"sourceRoot":""}]}