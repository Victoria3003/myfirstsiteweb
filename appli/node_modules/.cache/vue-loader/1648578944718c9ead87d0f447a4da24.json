quipement : <br />\n            {{ annonce.equipement }}\n          </p>\n        </div>\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo4.jpg\" />\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <div class=\"row featurette\">\n        <div class=\"col-sm\">\n          <img src=\"../assets/immo5.jpg\" />\n        </div>\n        <div class=\"col-sm\">\n          <p class=\"lead\">\n            Bilan Energetique :<br />\n            Consommation d'énérgie : {{ annonce.energie }} <br />\n            Emission de Gaz à effet de serre : {{ annonce.ges }}\n          </p>\n        </div>\n      </div>\n\n      <hr class=\"featurette-divider\" />\n\n      <!-- /END THE FEATURETTES -->\n    </div>\n    <!-- /.container -->\n  </main>\n</template>\n\n<script>\nimport { db } from \"../firebaseDb\";\n\nexport default {\n  data() {\n    return {\n      annonce: {},\n    };\n  },\n  created() {\n    let dbRef = db.collection(\"annonces\").doc(this.$route.params.id);\n    dbRef\n      .get()\n      .then((doc) => {\n        this.annonce = doc.data();\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n};\n</script>\n\n<style>\n.bd-placeholder-img {\n  font-size: 1.125rem;\n  text-anchor: middle;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n}\n\n@media (min-width: 768px) {\n  .bd-placeholder-img-lg {\n    font-size: 3.5rem;\n  }\n}\n</style>\n"]}]}