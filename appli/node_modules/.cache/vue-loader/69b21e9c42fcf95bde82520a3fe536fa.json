/label>\n            <input class=\"form-control\" id=\"cp\" v-model=\"annonce.cp\" required />\n          </div>\n          <div class=\"col-md-4 mb-2\">\n            <label>Surface</label>\n            <input\n              class=\"form-control\"\n              id=\"surface\"\n              v-model=\"annonce.surface\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Pieces</label>\n            <input\n              class=\"form-control\"\n              id=\"pieces\"\n              v-model=\"annonce.pieces\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Chambres</label>\n            <input\n              class=\"form-control\"\n              id=\"chambres\"\n              v-model=\"annonce.chambres\"\n              required\n            />\n          </div>\n          <div class=\"col-md-4 mb-3\">\n            <label>Année</label>\n            <input\n              class=\"form-control\"\n              id=\"annee\"\n              v-model=\"annonce.annee\"\n              required\n            />\n          </div>\n          <div class=\"col-md-6 mb-4\">\n            <label for=\"contactComment\">Descriptif :</label>\n            <textarea\n              class=\"form-control\"\n              id=\"contactComment\"\n              rows=\"3\"\n              v-model=\"annonce.commentaire\"\n              required\n            ></textarea>\n          </div>\n          <div class=\"col-md-6 mb-4\">\n            <label for=\"equipement\">Equipement :</label>\n            <textarea\n              class=\"form-control\"\n              id=\"equipement\"\n              rows=\"3\"\n              v-model=\"annonce.equipement\"\n              required\n            ></textarea>\n          </div>\n          <div class=\"col-md-4 mb-5\">\n            <label for=\"energie\">Energie</label>\n            <select\n              class=\"form-control\"\n              id=\"energie\"\n              v-model=\"annonce.energie\"\n              required\n            >\n              <option>A</option>\n              <option>B</option>\n              <option>C</option>\n              <option>D</option>\n              <option>E</option>\n              <option>F</option>\n              <option>G</option>\n            </select>\n          </div>\n          <div class=\"col-md-4 mb-5\">\n            <label for=\"ges\">Ges</label>\n            <select\n              class=\"form-control\"\n              id=\"ges\"\n              v-model=\"annonce.ges\"\n              required\n            >\n              <option>A</option>\n              <option>B</option>\n              <option>C</option>\n              <option>D</option>\n              <option>E</option>\n              <option>F</option>\n              <option>G</option>\n            </select>\n          </div>\n            <div class=\"col-md-4 mb-1\">\n            <label for=\"annonceStatut\">Statut</label>\n            <select\n              class=\"form-control\"\n              id=\"annonceStatut\"\n              v-model=\"annonce.statut\"\n              required\n            >\n              <option>A traiter</option>\n              <option>Traité</option>\n            </select>\n          </div>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Mettre a jour</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { db } from \"../firebaseDb\";\n\nexport default {\n  data() {\n    return {\n      annonce: {},\n    };\n  },\n  created() {\n    let dbRef = db.collection(\"annonces\").doc(this.$route.params.id);\n    dbRef\n      .get()\n      .then((doc) => {\n        this.annonce = doc.data();\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    onUpdateForm(event) {\n      event.preventDefault();\n      db.collection(\"annonces\")\n        .doc(this.$route.params.id)\n        .update(this.annonce)\n        .then(() => {\n          console.log(\"Annonce successfully updated!\");\n          this.$router.push(\"/adminAnnonces\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n"]}]}