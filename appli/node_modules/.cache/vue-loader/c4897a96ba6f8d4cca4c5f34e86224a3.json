 </div>\n\n      <button type=\"submit\" class=\"btn btn-dark btn-lg btn-block\">Se Connecter</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport firebase from \"firebase\";\n\nexport default {\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n  methods: {\n    userLogin() {\n      firebase\n        .auth()\n        .signInWithEmailAndPassword(this.user.email, this.user.password)\n        .then(() => {\n          this.$router.push(\"/admin\");\n        })\n        .catch((error) => {\n          alert(error.message);\n        });\n    },\n  },\n};\n</script>\n"]}]}